<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bjut.ssh.entity.Warehouseset" table="warehouseset" schema="material">
        <id name="warehouseCode" column="WarehouseCode"/>
        <property name="departmentId" column="DepartmentID"/>
        <property name="warehouseName" column="WarehouseName"/>
        <property name="guard" column="Guard"/>
        <property name="inspector" column="Inspector"/>
        <property name="initialFlag" column="InitialFlag"/>
    </class>
</hibernate-mapping>