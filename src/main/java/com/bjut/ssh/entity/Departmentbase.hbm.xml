<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bjut.ssh.entity.Departmentbase" table="departmentbase" schema="material">
        <id name="departmentId" column="DepartmentID"/>
        <property name="departmentName" column="DepartmentName"/>
        <property name="departmentProperty" column="DepartmentProperty"/>
        <property name="departmentLevel" column="DepartmentLevel"/>
        <property name="accountCategory" column="AccountCategory"/>
        <property name="accountItem" column="AccountItem"/>
        <property name="manufactureSystemDepartmentId" column="ManufactureSystemDepartmentID"/>
        <property name="allocationCoefficient" column="AllocationCoefficient"/>
    </class>
</hibernate-mapping>